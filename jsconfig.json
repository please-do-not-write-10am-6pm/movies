{
  "compilerOptions": {
    "baseUrl": "./src/",
    "jsx": "react",
    "paths": {
      "@/layout": [
        "app/components/layout"
      ],
      "@/layout/*": [
        "app/components/layout/*"
      ],
      "@/markup": [
        "app/components/markup"
      ],
      "@/pages": [
        "app/components/pages"
      ],
      "@/pages/*": [
        "app/components/pages/*"
      ],
      "@/constants/*": [
        "app/constants/*"
      ],
      "@/containers/*": [
        "app/containers/*"
      ],
      "@/contexts": [
        "app/contexts"
      ],
      "@/hocs/*": [
        "app/hocs/*"
      ],
      "@/redux/*": [
        "app/redux/*"
      ],
      "@/actions": [
        "app/redux/actions"
      ],
      "@/sagas/*": [
        "app/redux/sagas/*"
      ],
      "@/routing/*": [
        "app/routing/*"
      ],
      "@/types": [
        "app/types"
      ],
      "@/utils/*": [
        "app/utils/*"
      ],
      "@/i18n": [
        "app/i18n"
      ],
      "@/server/*": [
        "server/*"
      ]
    }
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}