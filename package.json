{
  "name": "movies",
  "version": "1.0.0",
  "description": "Application for movies search, based on Webpack 4 / Express / React + Redux-Saga / SSR",
  "main": "index.client.js",
  "scripts": {
    "lint": "eslint ./**",
    "lint:fix": "eslint ./** --fix",
    "dev:client": "cross-env NODE_ENV=development npx webpack-dev-server --config=configs/webpack.config.client.js --hot",
    "dev:server": "cross-env NODE_ENV=development npx webpack --config=configs/webpack.config.server.js -w",
    "dev": "npm run dev:client",
    "build:client": "cross-env NODE_ENV=production npx webpack --config=configs/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production npx webpack --config=configs/webpack.config.server.js",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nk11dev/movies.git"
  },
  "keywords": [
    "example",
    "webpack",
    "react",
    "redux",
    "redux-saga",
    "express",
    "SSR",
    "server side rendering",
    "movies"
  ],
  "author": "nk11dev",
  "license": "MIT",
  "homepage": "https://github.com/nk11dev/movies#readme",
