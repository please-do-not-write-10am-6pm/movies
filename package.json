{
  "name": "movies",
  "version": "0.0.1",
  "description": "Movies is a real world isomorphic application for movies search, based on Webpack 5 / Express / React 17 + Redux-Saga / Bootstrap 4.6 + CSS Modules / i18next / SSR",
  "main": "index.client.js",
  "scripts": {
    "cypress": "npx cypress open --config-file=cypress.json",
    "cypress:run": "npx cypress run --config-file=cypress.json",
    "test": "start-server-and-test dev:client http://localhost:8080 cypress:run",
    "lint": "npx eslint ./**",
    "lint:fix": "npx eslint ./** --fix",
    "dev:client": "cross-env NODE_ENV=development npx webpack serve --config=configs/webpack.config.client.js --hot",
    "dev:server": "cross-env NODE_ENV=development npx webpack --config=configs/webpack.config.server.js -w",
    "dev": "npm run dev:client",
    "build:client": "cross-env NODE_ENV=production npx webpack --config=configs/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production npx webpack --config=configs/webpack.config.server.js",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nk11dev/movies.git"
  },
  "keywords": [
    "example",
    "webpack",
    "react",
    "redux",
    "redux-saga",
    "express",
    "SSR",
    "server side rendering",
    "movies"
  ],
  "author": "nk11dev",
  "license": "MIT",
  "homepage": "https://github.com/nk11dev/movies#readme",
