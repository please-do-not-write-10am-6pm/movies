{
  "name": "webpack4__boilerplate",
  "version": "1.0.0",
  "description": "Repo with webpack 4 build boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config=configs/webpack.config.dev.js --hot",
    "build": "webpack --config=configs/webpack.config.prod.js",
    "serve": "http-server-spa ./dist/ index.html 8080",
    "prod": "npm run build && npm run serve",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nk11dev/webpack4__boilerplate.git"
  },
  "author": "nk11dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nk11dev/webpack4__boilerplate/issues"
  },
  "homepage": "https://github.com/nk11dev/webpack4__boilerplate#readme",
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@hot-loader/react-dom": "16.13.0",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "4.3.0",
    "file-loader": "6.1.0",
    "html-webpack-plugin": "4.4.1",
    "http-server-spa": "1.3.0",
    "mini-css-extract-plugin": "0.11.2",
    "node-sass": "4.14.1",
    "react-hot-loader": "4.12.21",
    "redux-logger": "3.0.6",
    "sass-loader": "10.0.2",
    "style-loader": "1.2.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.1.4"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5"
  }
}
