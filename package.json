{
  "name": "webpack4__boilerplate",
  "version": "1.0.0",
  "description": "Repo with webpack 4 build boilerplate",
  "main": "index.client.js",
  "config": {
    "API_PATH": "http://127.0.0.1:8081/api",
    "PORT_SERVER": "8081",
    "PORT_CLIENT_DEV": "8080",
    "RENDERING": "client"
  },
  "scripts": {
    "dev-client": "cross-env NODE_ENV=development npx webpack-dev-server --config=configs/webpack.config.client.js --hot",
    "build-client": "cross-env NODE_ENV=production npx webpack --config=configs/webpack.config.client.js",
    "dev-server": "cross-env NODE_ENV=development npx webpack --config=configs/webpack.config.server.js -w",
    "build-server": "cross-env NODE_ENV=production npx webpack --config=configs/webpack.config.server.js",
    "start-server": "node ./dist/server",
    "serve": "http-server-spa ./dist/client index.html 8080",
    "prod": "npm run build-client && npm run serve",
    "start": "npm run dev-client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nk11dev/webpack4__boilerplate.git"
  },
  "author": "nk11dev",
  "license": "MIT",
  "homepage": "https://github.com/nk11dev/webpack4__boilerplate#readme",
