{
  "name": "webpack4__boilerplate",
  "version": "1.0.0",
  "description": "Repo with webpack 4 build boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config=webpack.config.dev.js --open",
    "build": "webpack --config=webpack.config.prod.js",
    "serve": "npx http-server ./dist -p 8080 -o",
    "prod": "npm run build && npm run serve",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nk11dev/webpack4__boilerplate.git"
  },
  "author": "nk11dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nk11dev/webpack4__boilerplate/issues"
  },
  "homepage": "https://github.com/nk11dev/webpack4__boilerplate#readme",
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "4.3.0",
    "file-loader": "6.1.0",
    "html-webpack-plugin": "4.4.1",
    "http-server": "0.12.3",
    "mini-css-extract-plugin": "0.11.2",
    "node-sass": "4.14.1",
    "sass-loader": "10.0.2",
    "style-loader": "1.2.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.1.4"
  }
}
