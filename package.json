{
  "name": "movies",
  "version": "0.0.1",
  "description": "Movies is a real world isomorphic application for movies search, based on Webpack 5 / Express / React 17 + Redux-Saga / Bootstrap 4.6 + CSS Modules / i18next / SSR",
  "main": "index.client.js",
  "scripts": {
    "git:config": "git config core.hooksPath .githooks",
    "cypress": "npx cypress open --config-file=cypress.json",
    "cypress:run": "npx cypress run --config-file=cypress.json",
    "test": "start-server-and-test dev:client http://localhost:8080 cypress:run",
    "lint": "npx eslint ./**",
    "lint:fix": "npx eslint ./** --fix",
    "dev:client": "cross-env NODE_ENV=development cross-env RENDERING=client npx webpack serve --config=configs/webpack.config.client.js",
    "dev:server": "cross-env NODE_ENV=development npx webpack --config=configs/webpack.config.server.js -w",
    "dev": "npm run dev:client",
    "build:client": "cross-env NODE_ENV=production npx webpack --config=configs/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production npx webpack --config=configs/webpack.config.server.js",
    "analyze:client": "cross-env ANALYZE=true npm run build:client",
    "analyze:server": "cross-env ANALYZE=true npm run build:server",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/server.js",
    "prod": "cross-env RENDERING=server npm run build && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nk11dev/movies.git"
  },
  "keywords": [
    "example",
    "webpack",
    "react",
    "redux",
    "redux-saga",
    "express",
    "SSR",
    "server side rendering",
    "movies"
  ],
  "author": "nk11dev",
  "license": "MIT",
  "homepage": "https://github.com/nk11dev/movies#readme",
  "devDependencies": {
    "@babel/eslint-parser": "7.15.0",
    "@hot-loader/react-dom": "17.0.1",
    "cypress": "8.3.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-import-resolver-webpack": "0.13.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-react": "7.25.1",
    "start-server-and-test": "1.14.0",
    "webpack-dev-server": "4.1.0"
  },
